                    android:pathData="M 22 16.88 C 22 11.357 17.523 6.88 12 6.88 C 6.477 6.88 2 11.357 2 16.88"
                    android:fillAlpha="0"
                    android:strokeColor="#000000"
                    android:strokeWidth="1.67"
